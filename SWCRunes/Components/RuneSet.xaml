<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SWCRunes.Components.RuneSet"
             xmlns:lib="clr-namespace:SWCRunesLib;assembly=SWCRunesLib"
             xmlns:components="clr-namespace:SWCRunes.Components">
    <Border StrokeThickness="10">
    <VerticalStackLayout x:DataType="lib:RuneSet" BackgroundColor="AliceBlue">
        
        <components:Rune BindingContext="{Binding RuneOne}"/>
        
        <components:Rune BindingContext="{Binding RuneTwo}"/>
        
        <components:Rune BindingContext="{Binding RuneThree}"/>
        
        <components:Rune BindingContext="{Binding RuneFour}"/>
        
        <components:Rune BindingContext="{Binding RuneFive}"/>
        
        <components:Rune BindingContext="{Binding RuneSix}"/>
            
         </VerticalStackLayout>
        </Border>
</ContentView>

